{
  "name": "ng-closure-builder",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/andresdominguez/ng-closure-builder.git",
  "author": "Andres Dominguez",
  "license": "MIT",
  "dependencies": {
    "doctrine": "^2.0.0",
    "recast": "^0.11.20"
  },
  "devDependencies": {
    "mocha": "^3.2.0",
    "xo": "^0.17.1"
  },
  "scripts": {
    "test": "xo && mocha test/*test.js"
  },
  "xo": {
    "space": 2,
    "envs": [
      "mocha",
      "node"
    ],
    "ignores": [
      "test/file-before.js",
      "test/file-after.js"
    ],
    "rules": {
      "space-before-function-paren": 0,
      "unicorn/filename-case": 0,
      "comma-dangle": 0
    }
  }
}
